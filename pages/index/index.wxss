.cu-bar .search-form{height:100rpx;}
.cu-bar .icon-search{color:#000;margin: 0 1em 0 1.8em !important}
.txt-center{margin:0 auto}
.search-form .icon-camera{
  color:#000;margin: 0 1em 0 1.8em !important;font-size:38rpx;
}
.btn-camera{background:#333;
width:60px;
height:60px;margin:0 auto;
}
.btn-camera .icon-camera{
  margin:auto;
display:block;
height:30px;
width:30px;
font-size:30px;
line-height:58px;color:#fff
}
.cu-dialog{background:#fff;}
.photo-close{position:absolute;right:0;top:0;}
.coverimg{width:60px;margin:240px auto 0 auto;}
.cu-dialog image{display: block}
.minheight{min-height: 60rpx}
.txt-name{overflow: hidden;
text-overflow:ellipsis;
white-space: nowrap;}
.cu-btn.btn-share{width:100%;align-items:baseline;padding:0;background:#fff;height:auto}
.cu-btn.btn-share image{width:100%}

.cu-item [class*="categroy-"]{
